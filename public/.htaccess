AddHandler fastcgi-script .fcgi
Options +ExecCGI

RewriteEngine On
RewriteBase /

RewriteRule ^javascripts/tiny_mce/themes/advanced/(.*)$ - [L]
RewriteRule ^javascripts/tiny_mce/themes/simple/(.*)$ - [L]
RewriteRule ^javascripts/tiny_mce/plugins/(.*)$ - [L]

RewriteRule ^.+\.(JPG|JPEG|PNG|jpg|jpeg|gif|png|ico|css|js|swf|doc|pdf|DOC|PDF|txt|TXT|docx|DOCX|csv|CSV|xlsx|XLSX|xls|XLS|xml|XML|html|HTML|htm|HTM|opt|OPT) $0 [L]
RewriteRule ^(.*)$ dispatch.fcgi [QSA,L]

